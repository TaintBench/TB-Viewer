{
	"name": "TB-Viewer",
	"description": "A language server for TaintBench",
	"author": "Linghui Luo",
	"license": "EPL-2.0",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": ""
	},
	"publisher": "LinghuiLuo",
	"categories": [],
	"keywords": [
		"multi-root ready"
	],
	"engines": {
		"vscode": "^1.30.0"
	},
	"activationEvents": [
		"onLanguage:java"
	],
	"main": "./out/extension",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "taintbench",
					"title": "TaintBench",
					"icon": "media/bug.svg"
				}
			]
		},
		"views": {
			"taintbench": [
				{
					"id": "taintbench.groundtruth",
					"name": "Baseline Ground Truth"
				},
				{
					"id": "taintbench.aqlresults",
					"name": "AQL Results"
				},
				{
					"id": "taintbench.matchedresults",
					"name": "Matched Results"
				},
				{
					"id": "taintbench.unmatchedresults",
					"name": "Unmatched Results"
				}
			]
		},
		"menus": {
			"view/item/context": [
				{
					"command": "taintbench.goto",
					"group": "inline",
					"when": "view == taintbench.groundtruth"
				},
				{
					"command": "taintbench.goto",
					"group": "inline",
					"when": "view == taintbench.aqlresults"
				},
				{
					"command": "taintbench.goto",
					"group": "inline",
					"when": "view == taintbench.matchedresults"
				},
				{
					"command": "taintbench.goto",
					"group": "inline",
					"when": "view == taintbench.unmatchedresults"
				}
			]
		},
		"commands": [
			{
				"command": "taintbench.goto",
				"icon": "media/goto.svg",
				"title": "Goto"
			}
		],
		"configuration": {
			"type": "object",
			"title": "TaintBench",
			"properties": {
				"TaintBench.trace.server": {
					"scope": "window",
					"type": "string",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"default": "off",
					"description": "Traces the communication between VS Code and the language server."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "cp ../target/TB-Viewer-0.0.1-SNAPSHOT.jar TB-Viewer-0.0.1-SNAPSHOT.jar && npm run compile",
		"compile": "tsc -b",
		"watch": "tsc -b -w",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {
		"vscode-languageclient": "^5.2.1"
	},
	"devDependencies": {
		"vscode": "^1.1.33",
		"@types/mocha": "^5.2.6",
		"@types/node": "^11.13.0",
		"tslint": "^5.15.0",
		"typescript": "^3.4.2"
	}
}
